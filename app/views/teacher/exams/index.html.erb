<div class="container mt-10">
  <h3>All Exams</h3>
  <%= link_to  "Create New Exam", new_teacher_exam_path , class: 'btn btn-primary mb-4'%>
  <div class="row">
    <% @exams.each do |exam| %>
      <div class="col-md-4 mb-4">
        <%= render 'teacher/exams/teacher_admin_exam', exam: exam %>
      </div>
    <% end %>
  </div>
</div>